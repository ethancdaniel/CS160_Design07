    <string name="user1_tag">15 answers | Berkeley, CA</string>
    <string name="user1_reply">My dog did the same thing, it just takes time for them to adjust.</string>
    <string name="day10_update">Last updated 10 days ago</string>
    <string name="user2">CatCheese33</string>
    <string name="user2_tag">10 answers | Santa Rosa, CA</string>
    <string name="user2_reply">Have you been taking your dog on longer walks, he just might be tired.</string>
