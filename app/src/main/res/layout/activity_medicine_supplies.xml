<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".MedicineSupplies">


    <TextView
        android:id="@+id/essentialText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mb"
        android:text="Veterinarian Approved"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="87dp" />

    <TextView
        android:id="@+id/recommendedText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mb"
        android:text="At Home Treatment"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/checklistText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monr"
        android:text="Medicine Supplies Checklist"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="28dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/arrow_backward_ios"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="20dp" />


    <androidx.cardview.widget.CardView
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="4dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="126dp">

        <ImageView
            android:id="@+id/pushinP"
            android:layout_width="wrap_content"
            android:layout_height="17dp"
            android:layout_gravity="center_vertical|end"
            android:paddingEnd="8dp"
            app:srcCompat="@drawable/ic_baseline_local_parking_24" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <CheckBox
                android:id="@+id/pellets"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:button="@drawable/drawable"
                android:buttonTint="@color/primary"
                android:clickable="true"
                android:focusable="true"
                android:fontFamily="@font/monr"
                android:paddingStart="8dp"
                android:textSize="11sp"
                tools:text="Pellets">

            </CheckBox>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="4dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="165dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <CheckBox
                android:id="@+id/antibacterialFluid"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:button="@drawable/drawable"
                android:buttonTint="@color/primary"
                android:clickable="true"
                android:focusable="true"
                android:fontFamily="@font/monr"
                android:paddingStart="8dp"
                android:textSize="11sp"
                tools:text="Antibacterial Fluid ">

            </CheckBox>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="4dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="205dp">

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|end"
            android:paddingEnd="8dp"
            app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <CheckBox
                android:id="@+id/WaterTestKit"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:button="@drawable/drawable"
                android:buttonTint="@color/primary"
                android:clickable="true"
                android:focusable="true"
                android:fontFamily="@font/monr"
                android:paddingStart="8dp"
                android:textSize="11sp"
                tools:text="Water Test Kit">

            </CheckBox>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="4dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="245dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <CheckBox
                android:id="@+id/gfishprotect"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:button="@drawable/drawable"
                android:clickable="true"
                android:focusable="true"
                android:buttonTint="@color/primary"
                android:fontFamily="@font/monr"
                android:paddingStart="8dp"
                android:textSize="11sp"
                tools:text="API Goldfish Protect">

            </CheckBox>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="4dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="345dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <CheckBox
                android:id="@+id/warmWater"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:button="@drawable/drawable"
                android:clickable="true"
                android:focusable="true"
                android:buttonTint="@color/primary"
                android:fontFamily="@font/monr"
                android:paddingStart="8dp"
                android:textSize="11sp"
                tools:text="Warm Water">

            </CheckBox>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="4dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="385dp">

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|end"
            android:paddingEnd="8dp"
            app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <CheckBox
                android:id="@+id/salt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:button="@drawable/drawable"
                android:buttonTint="@color/primary"
                android:clickable="true"
                android:focusable="true"
                android:fontFamily="@font/monr"
                android:paddingStart="8dp"
                android:textSize="11sp"
                tools:text="Non-Iodized Salt">

            </CheckBox>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="21dp"
        android:layout_height="11dp"
        android:layout_gravity="center_vertical|end"
        app:srcCompat="@drawable/ic_baseline_local_parking_24"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="446dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="273dp"
        android:layout_height="59dp"
        android:backgroundTint="@color/primary"
        android:fontFamily="@font/monr"
        android:text="Buy Now"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="622dp" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monr"
        android:text="Prescription Required"
        android:textSize="8dp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="446dp" />


</androidx.constraintlayout.widget.ConstraintLayout>